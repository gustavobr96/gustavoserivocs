<!--begin::Body-->
<body id="kt_body" class="header-fixed header-mobile-fixed subheader-enabled sidebar-enabled page-loading">
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NM6QPT9D"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <!--begin::Main-->
    <div class="d-flex flex-column flex-root">
        <!--begin::Login-->
        <div class="login login-1 login-signin-on d-flex flex-column flex-lg-row flex-row-fluid bg-white" id="kt_login">
            <!--begin::Aside-->
            <div class="login-aside d-flex flex-row-auto bgi-size-cover bgi-no-repeat p-10 p-lg-10" style="background-image: url(assets/media/bg/bg-4.jpg);">
                <!--begin: Aside Container-->
                <div class="d-flex flex-row-fluid flex-column justify-content-between">
                    <!--begin: Aside header-->
                    <!--end: Aside header-->
                    <!--begin: Aside content-->
                    <div class="flex-column-fluid d-flex flex-column justify-content-center">
                        <h3 class="font-size-h1 mb-5 text-laranja">PREPARE-SE, em instantes você conhecerá um <b>NOVO MUNDO!</b> </h3>
                        <p class="font-weight-lighter opacity-80 text-laranja">Conectamos você aos melhores prestadores de serviço.</p>
                    </div>
                    <!--end: Aside content-->
                    <!--begin: Aside footer for desktop-->
                    <div class="d-none flex-column-auto d-lg-flex justify-content-between mt-10">
                        <div class="opacity-70 font-weight-bold text-laranja">© 2024 BICO</div>
                        <div class="d-flex">
                            <a target="_blank" href="@Url.Action("PoliticaPrivacidade", "Term")" class="text-laranja">Privacidade</a>
                            <a target="_blank" href="@Url.Action("TermoUso", "Term")" class="ml-10 text-laranja">Termos de uso</a>
                            <a class="ml-10 text-laranja">contato@bicoservicos.com.br</a>
                        </div>
                    </div>
                    <!--end: Aside footer for desktop-->
                </div>
                <!--end: Aside Container-->
            </div>
            <!--begin::Aside-->
            <!--begin::Content-->
            <div class="flex-row-fluid d-flex flex-column position-relative p-7 overflow-hidden">

                <!--begin::Content header-->
                <div class="position-absolute top-0 right-0 text-right mt-5 mb-15 mb-lg-0 flex-column-auto justify-content-center py-5 px-10">
                    <span class="font-weight-bold text-dark-50">Não tem uma conta ainda?</span>
                    <a href="javascript:;" class="font-weight-bold ml-2 text-laranja" id="kt_login_signup" style="font-weight: bold !important;">Cadastre-se</a>
                </div>
                <!--end::Content header-->
                <!--begin::Content body-->
                <div class="d-flex flex-column-fluid flex-center mt-30 mt-lg-0">
                    <!--begin::Signin-->
                    <div class="login-form login-signin">
                        <div class="d-flex flex-center mb-15">
                            <a>
                                <img src="assetsSite/img/gallery/logo.png"  alt="" />
                            </a>
                        </div>
                        <div class="text-center mb-10 mb-lg-20">
                            <h3 class="font-size-h1 text-laranja">Entrar Como Contratante</h3>
                            <p class="text-muted font-weight-bold">Digite seu e-mail e senha</p>
                        </div>
                        <!--begin::Form-->
                        <form class="form" novalidate="novalidate" action="Login/login-contratante" method="post">
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" asp-for="Email" type="text" placeholder="E-mail" name="email" autocomplete="off" />
                            </div>
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" asp-for="Password" type="password" placeholder="Senha" name="password" autocomplete="off" />
                            </div>
                            <!--begin::Action-->
                            <div class="form-group d-flex flex-wrap justify-content-between align-items-center">
                                <button id="kt_login_signin_submit" class="btn-custom w-100" onclick="showDiv()">Entrar</button>
                                <a href="javascript:;" class="text-dark-50 text-hover-primary my-3 mr-2" id="kt_login_forgot">Esqueceu a senha ?</a>
                            </div>
                            <!--end::Action-->
                        </form>
                        @if (!ViewData.ModelState.IsValid)
                        {
                            <div asp-validation-summary="All" class="text-danger validation-summary-errors"></div>
                        }
                        <!--end::Form-->
                    </div>
                    <!--end::Signin-->
                    <!--begin::Signup-->
                    <div class="login-form login-signup">
                        <div class="d-flex flex-center mb-15">
                            <a>
                                <img src="assetsSite/img/gallery/logo.png" class="max-h-75px" alt="" />
                            </a>
                        </div>
                        <div class="text-center mb-10 mb-lg-20">
                            <h3 class="font-size-h1">Inscrever-se</h3>
                            <p class="text-muted font-weight-bold">Insira seus dados para criar sua conta</p>

                        </div>
                        <!--begin::Form-->
                        <form class="form" novalidate="novalidate" action="Login/registerClient" method="post" id="form_login_signup">
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" type="text" asp-for="Name" placeholder="Nome" name="Name" autocomplete="off" />
                            </div>
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" type="text" asp-for="LastName" placeholder="Sobrenome" name="LastName" autocomplete="off" />
                            </div>
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" type="email" asp-for="Email" placeholder="E-mail" name="Email" autocomplete="off" />
                            </div>
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" onkeydown="javascript: fMasc( this, mCPF );" type="text" asp-for="CpfCnpj" placeholder="CPF" name="CpfCnpj" id="CpfCnpj" maxlength="14" autocomplete="off" />
                            </div>
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" onkeyup="handlePhone(event)" maxlength="14" type="text" asp-for="PhoneNumber" placeholder="Celular" name="PhoneNumber" autocomplete="off" minlength="12" maxlength="13" />
                            </div>
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6 password-field-minus-message" type="password" asp-for="Password" placeholder="Senha" name="Password" autocomplete="off" minlength="7" maxlength="10" />
                                <span id="password-status"></span>
                            </div>
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" type="password" asp-for="RPassword" placeholder="Repita a senha" name="RPassword" autocomplete="off" minlength="7" maxlength="10" />
                            </div>
                            <div class="form-group">
                                <label class="checkbox">
                                    <input type="checkbox" name="agree" />Eu concordo com os
                                    <a target="_blank" href="@Url.Action("TermoUso", "Term")"><b>termos e condições</b></a>.
                                    <span></span>
                                </label>
                            </div>
                            <div class="form-group d-flex flex-wrap flex-center">
                                <button id="kt_login_signup_submit" class="btn btn-primary font-weight-bold px-9 py-4 my-3 mx-4" value="validar">Cadastrar</button>
                                <button id="kt_login_signup_cancel" class="btn btn-light-primary font-weight-bold px-9 py-4 my-3 mx-4">Cancelar</button>
                            </div>
                        </form>
                        @if (!ViewData.ModelState.IsValid)
                        {
                            <div asp-validation-summary="All" class="text-danger validation-summary-errors"></div>
                        }
                        <!--end::Form-->
                    </div>
                    <!--end::Signup-->
                    <!--begin::Forgot-->
                    <div class="login-form login-forgot">
                        <div class="text-center mb-10 mb-lg-20">
                            <h3 class="font-size-h1">Esqueceu a senha ?</h3>
                            <p class="text-muted font-weight-bold">Insira seu e-mail para redefinir sua senha</p>
                        </div>
                        <!--begin::Form-->
                        <form class="form" novalidate="novalidate" novalidate="novalidate" action="Login/Forgot" method="post">
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" type="email" asp-for="Email" placeholder="E-mail" name="Email" autocomplete="off" />
                            </div>
                            <div class="form-group">
                                <input class="form-control form-control-solid h-auto py-5 px-6" onkeydown="javascript: fMasc( this, mCPF );" type="text" asp-for="CpfCnpj" placeholder="CPF" name="CpfCnpj" id="CpfCnpj" maxlength="14" autocomplete="off" />
                            </div>
                            <div class="form-group d-flex flex-wrap flex-center">
                                <button id="kt_login_forgot_submit" class="btn btn-primary font-weight-bold px-9 py-4 my-3 mx-4">Enviar</button>
                                <button id="kt_login_forgot_cancel" class="btn btn-light-primary font-weight-bold px-9 py-4 my-3 mx-4">Cancelar</button>
                            </div>
                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Forgot-->
                </div>
                <!--end::Content body-->
                <!--begin::Content footer for mobile-->
                <div class="d-flex d-lg-none flex-column-auto flex-column flex-sm-row justify-content-between align-items-center mt-5 p-5">
                    <div class="text-dark-50 font-weight-bold order-2 order-sm-1 my-2">© 2023 Bico</div>
                    <div class="d-flex order-1 order-sm-2 my-2">
                        <a class="text-dark-75 text-hover-primary">Privacidade</a>
                        <a class="text-dark-75 text-hover-primary ml-4">Legal</a>
                        <a class="text-dark-75 text-hover-primary ml-4">Contato</a>
                    </div>
                </div>
                <!--end::Content footer for mobile-->
            </div>
            <!--end::Content-->
        </div>
        <!--end::Login-->
    </div>

    <div class="modal fade" tabindex="-1" role="dialog" id="spinnerModal" data-backdrop="static">
        <div class="modal-dialog text-center" role="document" style="top:50%">
            <div style="text-align: center">
                <img src="~/assets/media/loading/loading.gif" alt="this slowpoke moves" width="80" />
            </div>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
    <script src="assets/password/jquery.strengthify.js"></script>
    <link rel="stylesheet" href="assets/password/strengthify.css" type="text/css">


    <script type="text/javascript">
        $('.password-field-minus-message').strengthify({
            zxcvbn: 'https://cdn.rawgit.com/dropbox/zxcvbn/master/dist/zxcvbn.js',
            "message": [
                "teste",

            ],
            drawTitles: false,
            drawMessage: false
        })
    </script>
